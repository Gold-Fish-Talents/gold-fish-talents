    "@nuxtjs/color-mode": "^4.0.0",
    "@nuxtjs/seo": "^3.2.2",
    "@nuxtjs/tailwindcss": "^6.14.0",
    "@pinia/nuxt": "^0.11.3",
    "@regle/nuxt": "^1.12.1",
    "@tresjs/cientos": "^5.1.1",
    "@tresjs/nuxt": "^5.1.1",
    "@tresjs/post-processing": "^3.1.1",
    "@types/web-push": "^3.6.4",
    "@unhead/vue": "^2.0.19",
    "@vite-pwa/nuxt": "^1.1.0",
    "@vueuse/components": "^14.1.0",
    "@vueuse/core": "^14.1.0",
    "@vueuse/nuxt": "^14.1.0",
    "date-fns": "^4.1.0",
    "notion-to-md": "^3.1.9",
    "nuxt": "^4.2.1",
    "nuxt-auth-utils": "^0.5.25",
    "nuxt-maplibre": "^1.2.0",
    "pinia": "^3.0.4",
    "three": "^0.181.2",
    "typesense": "^2.1.0",
    "uncrypto": "^0.1.3",
    "vue": "^3.5.25",
    "vue-router": "^4.6.3",
    "web-push": "^3.6.7",
    "zod": "^4.1.13"
