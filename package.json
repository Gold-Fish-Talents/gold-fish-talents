{
  "name": "gold-fish-talents",
  "version": "0.4.0",
  "description": "E2E platform empowering brands to discover, manage, and collaborate with creative talents",
  "author": "Shirsendu Bairagi <shirsendu2001@gmail.com>",
  "license": "GPL-3.0-only",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Algostract/gold-fish-talents.git"
  },
  "type": "module",
  "private": true,
  "engines": {
    "node": "^20.15.0 || ^22.11.0",
    "pnpm": "^9.15.0 || ^10.2.0",
    "bun": "^1.2.9"
  },
  "packageManager": "bun@1.3.3",
  "scripts": {
    "prepare": "husky || true",
    "postinstall": "nuxt prepare",
    "dev": "nuxt dev",
    "detect": "gitleaks git --verbose",
    "lint": "eslint . --fix",
    "format": "prettier . --write",
    "test:load": "k6 run tests/load.test.ts",
    "build": "nuxt build",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "start": "node --env-file=.env.prod .output/server/index.mjs",
    "clean": "nuxt cleanup && (cd src-tauri && cargo clean)",
    "docker:build": "docker build --build-arg VERSION=dev --build-arg BUILD_TIME=now -t gold-fish-talents:dev .",
    "docker:start": "docker run --detach --name gold-fish-talents-dev --env-file .env.prod -p 4000:3000 gold-fish-talents:dev",
    "tauri": "tauri",
    "tauri:dev": "tauri dev",
    "tauri:build": "tauri build",
    "tauri:dev:mobile": "tauri android dev",
    "tauri:build:mobile": "tauri android build"
  },
  "dependencies": {
    "@dotenvx/dotenvx": "^1.51.1",
    "@hannoeru/nuxt-otel": "^0.0.8",
    "@notionhq/client": "^4.0.2",
    "@nuxt/eslint": "^1.11.0",
    "@nuxt/fonts": "^0.12.1",
