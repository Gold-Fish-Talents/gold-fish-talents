  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@tauri-apps/cli": "^2.9.5",
    "@types/k6": "^1.4.0",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^9.39.1",
    "happy-dom": "^20.0.11",
    "husky": "^9.1.7",
    "jiti": "^2.6.1",
    "playwright-core": "^1.57.0",
    "prettier": "^3.7.4",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "typescript": "~5.9.3",
    "vitest": "^4.0.15"
